{
    "manifest_version": 3,
    "name": "Dominion Timer",
    "version": "0.1.0",
    "description": "Add player timers to dominion.games",
    "content_scripts": [
      {
        "matches": ["https://dominion.games/*"], 
        "js": [
          "jquery-3.7.1.min.js",
          "content.js"
        ]
      }
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": []
  }